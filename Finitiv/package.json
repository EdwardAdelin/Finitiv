{
  "name": "finitiv-portfolio",
  "version": "1.0.0",
  "description": "Portfolio website for Finitiv Photography",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon --watch server.js --inspect=0 server.js\" \"(cd client && npm run dev)\"",
    "client": "cd client && npm run dev",
    "build-client": "cd client && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "body-parser": "^1.20.2"
  }
  ,
  "devDependencies": {
    "concurrently": "^8.0.0",
    "nodemon": "^2.0.22"
  }
}